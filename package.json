{
  "name": "app.calculators",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu ts-node --project tsconfig.server.json server.ts",
    "dev-no-custom-server": "next dev",
    "build": "npm run extract:i18n && npm run compile:i18n && next build && tsc -p tsconfig.server.json",
    "export": "npm run build && next export",
    "start": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu node dist/server",
    "start-no-custom-server": "next start",
    "extract:i18n": "formatjs extract '{pages,components}/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang/ compiled-lang/",
    "type-check": "tsc",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@formatjs/cli": "4.8.2",
    "@formatjs/intl-datetimeformat": "3.3.5",
    "@formatjs/intl-getcanonicallocales": "1.9.0",
    "@formatjs/intl-numberformat": "6.2.10",
    "@formatjs/intl-pluralrules": "4.3.2",
    "@formatjs/intl-relativetimeformat": "8.1.8",
    "@formatjs/intl-utils": "3.8.4",
    "@infinitetoolbox/bmi-calculator": "0.2.14",
    "@infinitetoolbox/vat-calculator": "0.2.15",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "accepts": "1.3.8",
    "babel-plugin-react-intl": "8.2.25",
    "clsx": "1.1.1",
    "cross-env": "7.0.3",
    "full-icu": "1.4.0",
    "glob": "8.0.3",
    "intl": "1.2.5",
    "intl-locales-supported": "1.8.12",
    "mathjs": "10.1.1",
    "next": "10.2.3",
    "next-pwa": "5.4.4",
    "next-seo": "4.29.0",
    "next-sitemap": "2.4.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.6"
  },
  "devDependencies": {
    "@emmanuelgautier/renovate-config": "1.5.0",
    "@types/accepts": "1.3.5",
    "@types/glob": "7.2.0",
    "@types/mathjs": "6.0.12",
    "@types/node": "14.18.12",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "cross-spawn": "7.0.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  }
}
