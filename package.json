{
  "name": "app.calculators",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu ts-node --project tsconfig.server.json server.ts",
    "dev-no-custom-server": "next dev",
    "build": "npm run extract:i18n && npm run compile:i18n && next build && tsc -p tsconfig.server.json",
    "export": "npm run build && next export",
    "start": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu node dist/server",
    "start-no-custom-server": "next start",
    "extract:i18n": "formatjs extract '{pages,components}/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang/ compiled-lang/",
    "type-check": "tsc",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@formatjs/cli": "2.13.8",
    "@formatjs/intl-datetimeformat": "2.8.1",
    "@formatjs/intl-getcanonicallocales": "1.5.1",
    "@formatjs/intl-numberformat": "5.7.3",
    "@formatjs/intl-pluralrules": "3.5.3",
    "@formatjs/intl-relativetimeformat": "7.3.3",
    "@formatjs/intl-utils": "3.8.4",
    "@infinitetoolbox/bmi-calculator": "0.2.0",
    "@infinitetoolbox/vat-calculator": "0.2.2",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "accepts": "1.3.7",
    "babel-plugin-react-intl": "8.2.13",
    "clsx": "1.1.1",
    "cross-env": "7.0.2",
    "full-icu": "1.3.1",
    "glob": "7.1.6",
    "intl": "1.2.5",
    "intl-locales-supported": "1.8.12",
    "mathjs": "8.0.1",
    "next": "10.0.3",
    "next-pwa": "3.1.5",
    "next-seo": "4.16.0",
    "next-sitemap": "1.2.26",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intl": "5.9.2"
  },
  "devDependencies": {
    "@emmanuelgautier/renovate-config": "1.1.2",
    "@types/accepts": "1.3.5",
    "@types/glob": "7.1.3",
    "@types/mathjs": "6.0.9",
    "@types/node": "14.14.6",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "cross-spawn": "7.0.3",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  }
}
